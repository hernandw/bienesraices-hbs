{{> menu_cat}}
<section class="py-5">

    <h2 class="text-center text-4xl font-extrabold pt-10">{{title}}</h2>
    {{#if properties}}
    <div class="grid grid-cols-1 lg:grid-cols-3 gap-5 mt-14 px-5">

        {{#each properties}}
        {{> card}}
        {{/each}}
        {{else}}

        <p class="text-center mt-10 text-xl">No hay resultados</p>
        {{/if}}

    </div>

</section>